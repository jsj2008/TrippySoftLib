<?xml version="1.0" encoding="utf-8" ?> 
<application xmlns="http://ns.adobe.com/air/application/1.5"> 
    <id>com.luaye.console.RemoteAIR</id> 
    <version>2.31</version> 
    <filename>ConsoleRemote</filename> 
    <name>Console Remote</name> 
    <description> 
        <text xml:lang="en">flash-console Remote</text> 
    </description> 
    <copyright>Lu Aye Oo (c) 2009</copyright> 
    <initialWindow> 
